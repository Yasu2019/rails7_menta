<?xml version="1.0"?>
<?mso-application progid="Excel.Sheet"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
 xmlns:o="urn:schemas-microsoft-com:office:office"
 xmlns:x="urn:schemas-microsoft-com:office:excel"
 xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
 xmlns:html="http://www.w3.org/TR/REC-html40">
 <DocumentProperties xmlns="urn:schemas-microsoft-com:office:office">
  <Author>mec21</Author>
  <LastAuthor>mec21</LastAuthor>
  <LastPrinted>2022-02-13T04:43:10Z</LastPrinted>
  <Created>2022-02-13T04:23:55Z</Created>
  <Version>16.00</Version>
 </DocumentProperties>
 <OfficeDocumentSettings xmlns="urn:schemas-microsoft-com:office:office">
  <AllowPNG/>
 </OfficeDocumentSettings>
 <ExcelWorkbook xmlns="urn:schemas-microsoft-com:office:excel">
  <WindowHeight>8145</WindowHeight>
  <WindowWidth>20490</WindowWidth>
  <WindowTopX>0</WindowTopX>
  <WindowTopY>0</WindowTopY>
  <ProtectStructure>False</ProtectStructure>
  <ProtectWindows>False</ProtectWindows>
 </ExcelWorkbook>
 <Styles>
  <Style ss:ID="Default" ss:Name="Normal">
   <Alignment ss:Vertical="Center"/>
   <Borders/>
   <Font ss:FontName="游ゴシック" x:CharSet="128" x:Family="Modern" ss:Size="11"
    ss:Color="#000000"/>
   <Interior/>
   <NumberFormat/>
   <Protection/>
  </Style>
  <Style ss:ID="s62">
   <Font ss:FontName="メイリオ" x:CharSet="128" x:Family="Modern" ss:Size="11"
    ss:Color="#000000"/>
  </Style>
  <Style ss:ID="s63">
   <Font ss:FontName="メイリオ" x:CharSet="128" x:Family="Modern" ss:Size="18"
    ss:Color="#000000"/>
  </Style>
  <Style ss:ID="s65">
   <Alignment ss:Horizontal="Right" ss:Vertical="Center"/>
   <Borders>
    <Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
   </Borders>
   <Font ss:FontName="メイリオ" x:CharSet="128" x:Family="Modern" ss:Size="11"
    ss:Color="#000000"/>
   <NumberFormat ss:Format="Short Date"/>
  </Style>
  <Style ss:ID="s67">
   <Borders>
    <Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
   </Borders>
   <Font ss:FontName="メイリオ" x:CharSet="128" x:Family="Modern" ss:Size="11"
    ss:Color="#000000"/>
  </Style>
  <Style ss:ID="s68">
   <Borders>
    <Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
    <Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
   </Borders>
   <Font ss:FontName="メイリオ" x:CharSet="128" x:Family="Modern" ss:Size="11"
    ss:Color="#000000"/>
   <NumberFormat ss:Format="Short Date"/>
  </Style>
 </Styles>
 <Worksheet ss:Name="Sheet1">
  <Table  x:FullColumns="1"
   x:FullRows="1" ss:StyleID="s62" ss:DefaultColumnWidth="54"
   ss:DefaultRowHeight="18.75">
   <Column ss:StyleID="s62" ss:Width="36.75"/>
   <Column ss:StyleID="s62" ss:Width="212"/>
   <Column ss:StyleID="s62" ss:Width="150"/>
   <Column ss:StyleID="s62" ss:Width="66.75"/>
   <Column ss:StyleID="s62" ss:Width="310"/>
   <Column ss:StyleID="s62" ss:Width="150"/>
   <Column ss:StyleID="s62" ss:Width="69.75"/>
   <Column ss:StyleID="s62" ss:Width="66"/>
   <Column ss:StyleID="s62" ss:Width="69.75"/>
   <Column ss:StyleID="s62" ss:Width="42.75"/>
   <Column ss:StyleID="s62" ss:Width="66"/>
   <Row ss:AutoFitHeight="0" ss:Height="28.5">
    <Cell ss:StyleID="s63"><PhoneticText
      xmlns="urn:schemas-microsoft-com:office:excel">セイヒントウロクイチラン</PhoneticText><Data
      ss:Type="String">IATF製品登録データ一覧</Data></Cell>
    <Cell ss:Index="9" ss:MergeAcross="2" ss:StyleID="s65" ss:Formula="=TODAY()"><Data
      ss:Type="DateTime">2022-02-13T00:00:00.000</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0">
    <Cell ss:StyleID="s67"><Data ss:Type="String">ID</Data></Cell>
    <Cell ss:StyleID="s67"><PhoneticText
      xmlns="urn:schemas-microsoft-com:office:excel">ズバン</PhoneticText><Data
      ss:Type="String">図番</Data></Cell>
    <Cell ss:StyleID="s67"><PhoneticText
      xmlns="urn:schemas-microsoft-com:office:excel">ザイリョウ</PhoneticText><Data
      ss:Type="String">材料コード</Data></Cell>
    <Cell ss:StyleID="s67"><Data ss:Type="String">フェーズ</Data></Cell>
    <Cell ss:StyleID="s67"><PhoneticText
      xmlns="urn:schemas-microsoft-com:office:excel">コウモク</PhoneticText><Data
      ss:Type="String">項目</Data></Cell>
    <Cell ss:StyleID="s67"><Data ss:Type="String">コメント</Data></Cell>
    <Cell ss:StyleID="s67"><PhoneticText
      xmlns="urn:schemas-microsoft-com:office:excel">トウロクヒ</PhoneticText><Data
      ss:Type="String">登録日</Data></Cell>
    <Cell ss:StyleID="s67"><PhoneticText
      xmlns="urn:schemas-microsoft-com:office:excel">カンリョウヨテイヒ</PhoneticText><Data
      ss:Type="String">完了予定日</Data></Cell>
    <Cell ss:StyleID="s67"><PhoneticText
      xmlns="urn:schemas-microsoft-com:office:excel">カンリョウヒ</PhoneticText><Data
      ss:Type="String">完了日</Data></Cell>
    <Cell ss:StyleID="s67"><PhoneticText
      xmlns="urn:schemas-microsoft-com:office:excel">タッセイド</PhoneticText><Data
      ss:Type="String">達成度</Data></Cell>
    <Cell ss:StyleID="s67"><Data ss:Type="String">ステイタス</Data></Cell>
   </Row>

   <% @products.each do |pro| %>
     <% if pro.documenttype=="製品"%>
       <Row ss:AutoFitHeight="0">
        <Cell ss:StyleID="s67"><Data ss:Type="Number"><%= pro.id %></Data></Cell>
        <Cell ss:StyleID="s67"><Data ss:Type="String"><%= pro.partnumber %></Data></Cell>
        <Cell ss:StyleID="s67"><Data ss:Type="String"><%= pro.materialcode %></Data></Cell>
        <Cell ss:StyleID="s67"><Data ss:Type="String"><%= pro.phase %></Data></Cell>
        <Cell ss:StyleID="s67"><Data ss:Type="String"><%= pro.food %></Data></Cell>
        <Cell ss:StyleID="s67"><Data ss:Type="String"><%= pro.description %></Data></Cell>
        <Cell ss:StyleID="s68"><Data ss:Type="String"><%= pro.start_time.strftime('%y/%m/%d') %></Data></Cell>
        <Cell ss:StyleID="s68"><Data ss:Type="String"><%= pro.deadline_at.strftime('%y/%m/%d') %></Data></Cell>
        <Cell ss:StyleID="s68"><Data ss:Type="String"><%= pro.end_at.strftime('%y/%m/%d') %></Data></Cell>
        <Cell ss:StyleID="s67"><Data ss:Type="Number"><%= pro.goal_attainment_level %></Data></Cell>
        <Cell ss:StyleID="s67"><Data ss:Type="String"><%= pro.status %></Data></Cell>
       </Row>
     <% end %>    
   <% end %>

  </Table>
  <WorksheetOptions xmlns="urn:schemas-microsoft-com:office:excel">
   <PageSetup>
    <Layout x:Orientation="Landscape"/>
    <Header x:Margin="0.31496062992125984"/>
    <Footer x:Margin="0.31496062992125984" x:Data="&amp;R&amp;P"/>
    <PageMargins x:Bottom="0.74803149606299213" x:Left="0.23622047244094491"
     x:Right="0.23622047244094491" x:Top="0.74803149606299213"/>
   </PageSetup>
   <Unsynced/>
   <Print>
    <ValidPrinterInfo/>
    <PaperSizeIndex>9</PaperSizeIndex>
    <Scale>93</Scale>
    <HorizontalResolution>1200</HorizontalResolution>
    <VerticalResolution>1200</VerticalResolution>
   </Print>
   <ShowPageBreakZoom/>
   <Selected/>
   <Panes>
    <Pane>
     <Number>3</Number>
     <ActiveRow>11</ActiveRow>
     <ActiveCol>4</ActiveCol>
    </Pane>
   </Panes>
   <ProtectObjects>False</ProtectObjects>
   <ProtectScenarios>False</ProtectScenarios>
  </WorksheetOptions>
 </Worksheet>
</Workbook>
